#include <stdio.h>
void addNumbers();         // function prototype

void main()
{

    addNumbers();        // function call
}

void addNumbers()         // function definition
{
    int n1, n2, result;

    printf("Enters two numbers: ");
    scanf_s("%d %d",&n1,&n2);

    result = n1+n2;
    printf("%d", result);                  // return statement
}
