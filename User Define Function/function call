//#2->  Catch the return value in main function in a variable 
#include<stdio.h>
#include<conio.h>
//No need to add function Prototype, Because we write the functon at the top of the code.
int AddNum()
{
	int x, y, sum;
	printf("Enter two number: ");
	scanf_s("%d%d", &x, &y);
	sum = x + y;
	return sum;
}
int main()
{
	int result = AddNum(); // Here AddNum function return sum value. And we store it in result
	printf("Result = %d", result);
}






// parameret pass value from main to user define function
