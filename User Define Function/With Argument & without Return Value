#include<stdio.h>
#include<conio.h>

void AddNum(int x, int y);

void main()
{
	int a, b, result;
	printf("Enter two numbers: ");
	scanf_s("%d%d", &a, &b);
	AddNum(a, b);                   // parameter pass the values;
}

void AddNum(int x, int y)
{
	int sum;
	sum = x + y;
	printf("Sum of two numbers: %d", sum);
}
