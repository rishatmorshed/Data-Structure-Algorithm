#include<stdio.h>
#include<conio.h>

int AddNum();

void main()
{
	int result;
	result = AddNum();          // store return value in result and call function
	printf("Sum = %d", result);
}

int AddNum()
{
	int a, b, sum;
	printf("Enter two number: ");
	scanf_s("%d%d", &a, &b);
	sum = a + b;
	return sum;  //return value of sum
}
