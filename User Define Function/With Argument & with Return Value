#include<stdio.h>
#include<conio.h>

int AddNum(int x, int y);

void main()
{
	int a, b, result;
	printf("Enter two numbers: ");
	scanf_s("%d%d", &a, &b);
	 result = AddNum(a, b);
	 printf("Sum of %d and %d is = %d", a, b, result);
}

int AddNum(int x, int y)
{
	int sum;
	sum = x + y;
	return sum;
}
